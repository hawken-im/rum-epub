{
  "version": "0.0.16",
  "name": "rum-epub-app",
  "productName": "Rum Epub",
  "description": "Rum Epub Desktop App",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noe132/rum-epub.git"
  },
  "author": {
    "name": "rumsystem.net",
    "email": "dev@press.one"
  },
  "license": "MIT",
  "homepage": "https://github.com/noe132/rum-epub#readme",
  "bugs": {
    "url": "https://github.com/noe132/rum-epub/issues"
  },
  "scripts": {
    "start": "node ./build/scripts/CheckPortInUse.js && concurrently \"yarn start:main\" \"cross-env yarn start:renderer\"",
    "start:main": "cross-env NODE_ENV=development electron .",
    "start:renderer": "vite",
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "rimraf ./src/main_dist && tsc --project src/main/tsconfig.json",
    "build:staging": "cross-env BUILD_ENV=staging yarn build:renderer",
    "build:renderer": "tsc && vite build",
    "build:quorum": "git submodule update --init && git submodule foreach git pull origin main && ./scripts/build_quorum.sh",
    "a": "cross-env analyze=true vite build",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint": "cross-env NODE_ENV=development eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "package": "yarn build && yarn package:all",
    "package:staging:all": "yarn build:staging && yarn package:all",
    "package:all": "yarn package:mac && yarn package:win && yarn package:linux && node ./scripts/clear_after_build.js",
    "package:mac": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --mac --publish never",
    "package:win": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --win --ia32 --publish never",
    "package:linux": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --linux --publish never",
    "package:release": "yarn build && yarn package:release:all",
    "package:release:all": "rm -rf release && yarn package:release:mac && yarn package:release:win && node ./scripts/clear_after_build.js",
    "package:release:mac": "env-cmd electron-builder build --mac --publish never",
    "package:release:win": "env-cmd electron-builder build --win --ia32 --publish never",
    "package:release:linux": "env-cmd electron-builder build --linux --publish never",
    "package:release:win64": "env-cmd electron-builder build --win --publish never",
    "postinstall": "rimraf ./node_modules/.cache/"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@mui/styles": "^5.6.2",
    "boxicons": "^2.1.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "dexie": "^3.2.1",
    "electron-dl": "^3.3.1",
    "env-cmd": "^10.1.0",
    "epubjs": "^0.3.93",
    "escape-string-regexp": "^5.0.0",
    "fp-ts": "^2.11.10",
    "fs-extra": "^10.1.0",
    "get-port": "^5.1.1",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "node-watch": "^0.7.3",
    "query-string": "^7.0.1",
    "react": "^18.0.0",
    "react-avatar-editor": "^12.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-hook": "^4.0.3",
    "react-refresh": "^0.12.0",
    "scroll-into-view-if-needed": "^2.2.29",
    "tar-stream": "^2.2.0",
    "toml": "^3.0.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@noe132/eslint-config-react": "^0.1.4",
    "@svgr/core": "^6.2.1",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-avatar-editor": "^12.0.0",
    "@types/react-dom": "^18.0.1",
    "@types/rimraf": "^3.0.2",
    "@types/tar-stream": "^2.2.2",
    "@types/yauzl": "^2.10.0",
    "@vitejs/plugin-react": "^1.3.1",
    "chalk": "^4.1.2",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.2",
    "electron": "18.1.0",
    "electron-builder": "^23.0.3",
    "electron-builder-notarize": "^1.4.0",
    "electron-log": "^4.4.6",
    "electron-rebuild": "^3.2.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "eslint": "^8.13.0",
    "eslint-watch": "^8.0.0",
    "module-alias": "^2.2.2",
    "postcss": "^8.4.12",
    "rimraf": "^3.0.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.50.1",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-commonjs-externals": "^0.1.1"
  },
  "devEngines": {
    "node": ">=12.13.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  }
}
